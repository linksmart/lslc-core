{
  "id": "demo-dgw",
  "description": "Flex4grid device gateway - prepilot",
  "publicEndpoint": "http://localhost:8080",
  "staticDir": "./static",
  "http": {
    "bindAddr": "0.0.0.0",
    "bindPort": 8080
  },
  "protocols": {
    "REST": {
      "location": "/rest"
    },
    "MQTT": {
      "url": "tcp://si.broker.flex4grid.eu:1883",
      "prefix": "/demo-dgw",
      "DisconnectTopic": "DisconnectTopic",
      "DisconnectMessage": "DisconnectMessage",
      "DisconnectedRetained": true,
      "QoS": 0
    }
  }
}

