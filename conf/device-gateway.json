{
  "id": "demo-dgw",
  "description": "Example Gateway",
  "publicEndpoint": "http://fqdn-of-the-host:8080",
  "staticDir": "./static",
  "catalog": [
    {
      "discover": true,
      "endpoint": "http://localhost:8080/rc"
    }
  ],
  "http": {
    "bindAddr": "0.0.0.0",
    "bindPort": 8080
  },
  "protocols": {
    "REST": {
      "location": "/rest"
    },
    "MQTT": {
      "url": "tcp://127.0.0.1:1883",
      "prefix": "/demo-dgw"
    }
  }
}
